<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">NYTimes Movie Reviews</string>
    <string name="api_root_url">http://api.nytimes.com/svc/movies/v2/reviews/</string>
    <string name="ordered_by_date">by-date</string>
    <string name="data_updating">Fetching movie reviews</string>
    <string name="data_fetch_error">Unable to get reviews!</string>

    <!-- Date format in days -->
    <string name="date_format_days">
        <xliff:g id="days">%1$1.0f</xliff:g>d
    </string>

    <!-- Date format in hours -->
    <string name="date_format_hours">
        <xliff:g id="hours">%1$1.0f</xliff:g>h
    </string>

    <!-- Date format in mins -->
    <string name="date_format_mins">
        <xliff:g id="mins">%1$1.0f</xliff:g>m
    </string>

</resources>
